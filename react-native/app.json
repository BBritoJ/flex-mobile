{
  "expo": {
    "name": "Flex Mobile",
    "slug": "flex-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-twilio.png",
    "splash": {
      "image": "./assets/splash-twilio.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-twilio.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.bruno222.flexmobile",
      "googleServicesFile": "./google-services.json",
      "permissions": ["NOTIFICATIONS"],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
